## emti
# glyphs
p := [ bilabial    , manner=stop        , voice=unvoiced ]
b := [ bilabial    , manner=stop        , voice=voiced   ]
t := [ alveolar    , manner=stop        , voice=unvoiced ]
d := [ alveolar    , manner=stop        , voice=voiced   ]
k := [ velar       , manner=stop        , voice=unvoiced ]
g := [ velar       , manner=stop        , voice=voiced   ]
ʔ := [ glottal     , manner=stop        , voice=unvoiced ]
m := [ bilabial    , manner=nasal       , voice=voiced   ]
n := [ alveolar    , manner=nasal       , voice=voiced   ]
ɾ := [ alveolar    , manner=tap         , voice=voiced   ]
f := [ labiodental , manner=fricative   , voice=unvoiced ]
v := [ labiodental , manner=fricative   , voice=voiced   ]
s := [ alveolar    , manner=fricative   , voice=unvoiced ]
z := [ alveolar    , manner=fricative   , voice=voiced   ]
x := [ velar       , manner=fricative   , voice=unvoiced ]
ɣ := [ velar       , manner=fricative   , voice=voiced   ]
h := [ glottal     , manner=fricative   , voice=unvoiced ]
ʋ := [ labiodental , manner=approximant , voice=voiced   ]
ɹ := [ alveolar    , manner=approximant , voice=voiced   ]
ɰ := [ velar       , manner=approximant , voice=voiced   ]

i := [ height = high , ~round , syllabic ]
u := [ height = high ,  round , syllabic ]
a := [ height = low  , ~round , syllabic ]


# categories
C := p,t,k,ʔ,m,n,ɾ,f,s,x,h,ʋ,ɹ,ɰ
